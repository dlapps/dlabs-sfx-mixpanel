{
  "name": "dlapps/mixpanel-bundle",
  "description": "Manages integration with the MixPanel system",
  "type": "symfony-bundle",
  "homepage": "https://www.dreamlabs.ro",
  "authors": [
    {
      "name": "Petre Pătrașc",
      "email": "petre@dreamlabs.ro",
      "homepage": "https://www.dreamlabs.ro"
    },
    {
      "name": "Dreamlabs Team",
      "email": "office@dreamlabs.ro",
      "homepage": "https://www.dreamlabs.ro"
    }
  ],
  "require": {
    "php": "^7.1",
    "mixpanel/mixpanel-php": "^2.6",
    "symfony/symfony": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "DL\\MixpanelBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DL\\MixpanelBundle\\Tests\\": "tests/"
    }
  },
  "license": "proprietary",
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "squizlabs/php_codesniffer": "^2.7"
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
  },
  "config": {
    "sort-packages": true
  }
}
